    "@typescript-eslint/parser": "^8.54.0",
    "cypress": "^15.10.0",
    "eslint": "^9.39.2",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "body-parser": "^2.2.2",
