  "scripts": {
    "test": "cypress run",
    "test:open": "cypress open",
    "lint": "eslint src/**/*.ts cypress/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"cypress/**/*.ts\"",
    "demo:start": "node demo-app/server.js",
    "ai:generate": "node --env-file=.env -r ts-node/register src/cli/generate-tests.ts",
    "validate:tests": "node --env-file=.env -r ts-node/register src/cli/validate-tests.ts",
    "confidence:calculate": "node --env-file=.env -r ts-node/register src/cli/calculate-confidence.ts",
    "gates:validate": "node --env-file=.env -r ts-node/register src/cli/validate-gates.ts",
    "report:rejection": "node --env-file=.env -r ts-node/register src/cli/rejection-report.ts",
    "report:coverage": "node --env-file=.env -r ts-node/register src/cli/coverage-report.ts",
    "report:dashboard": "node --env-file=.env -r ts-node/register src/cli/generate-dashboard.ts",
    "report:serve": "node --env-file=.env -r ts-node/register src/cli/serve-dashboard.ts"
  },
  "keywords": [],
